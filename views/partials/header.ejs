<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="/stylesheets/styles.css">
</head>
<body>
  <nav>
    <h1><a href="/">My JWT authentication</a></h1>
    <ul>
        <% if(userID=="Guest") { %>
        <li>Guest</li>
        <li><a href="/auth/login">Log in</a></li>
        <li><a href="/auth/register" class="btn">Sign in</a></li>
        <% } else { %>
        <li><%=userID%></li>
        <li><a href="/auth/logout" class="btn">Log out</a></li>
        <% } %>
    </ul>
  </nav>
